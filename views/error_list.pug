extends layout 

block content
      table(class='table table-striped')
        thead(style='height: 65px;' class="thead-light")
            th.u-table-cell #
            th.u-table-cell Id
            th.u-table-cell Script
            th.u-table-cell Date
            th.u-table-cell action
        tbody
          each val,index in list
            tr(style='height: 65px;')
              td.u-table-cell=index+1
              td.u-table-cell=val._id 
              td.u-table-cell=val.script
              td.u-table-cell=val.date
              td.u-table-cell
                button(href='/log' type="button" id="errorbtn" onclick='showLog('+'"'+ val.log +'"'+')' class="btn btn-warning btn-sm m-1") log
                button(href='/log' type="button" id="errorbtn" onclick="showLog("+"'"+ JSON.stringify(val.data) +"'"+")" class="btn btn-warning btn-sm m-1") Data
                button(href='' type="button" onclick='deleteE('+'"'+ val._id +'"'+')' class="btn btn-danger btn-sm m-1") delete
                buttom(href='' type="button" class="btn btn-info btn-sm m-1") run again
block content_scripts
  script(src="/javascripts/error.js") 
